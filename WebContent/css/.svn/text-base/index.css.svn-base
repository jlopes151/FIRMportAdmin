body 
{
	margin: 0;
	padding: 0;
	background:#f2f2f6 url('background.jpg') no-repeat;
	font: 12px Myriad,Helvetica,Tahoma,Arial,clean,sans-serif; 
	font-size: 100%;
}

h1,h4	
{ 
	color: #999;
	margin-bottom: 0; 
    margin-left:25px;
}

p	
{ 
	color: #999; 
	margin-top: .5em;
	font-size: .75em;
	padding-left:15px;	
}

#navigation-block {
	position:absolute;
	top:80px;
	left:25px;
	width:25%
}

#field-block{
	position: absolute;
	top:115px;
	left:300px;
	background: #FFFFFF;
	color: #999;
	width: 80%;
	height:85%;
}

ul#subMenu1,
ul#subMenu2,
ul#subMenu3,
ul#subMenu4,
ul#subMenu5,
ul#subMenu6,
ul#subMenu7,
ul#subMenu8,
ul#subMenu9,
ul#subMenu10,
ul#subMenu11,
ul#subMenu12,
ul#subMenu13,
ul#subMenu14,
ul#subMenu15{
	display:block;
}

#hide {
	position:absolute;
	top:30px;
	left:-200px;
}

ul#sliding-navigation
{
	list-style: none;
	font-size: .75em;
	margin: 30px 0;
	padding: 0;
}

ul#sliding-navigation li.sliding-element h3,
ul#sliding-navigation li.sliding-element a,
ul#sliding-navigation li.sliding-element p
ul#sliding-navigation div
{
	display: block;
	width: 150px;
	padding: 5px 18px;
	margin: 0;
	margin-bottom: 5px;
}

ul#sliding-navigation li.sliding-element h3
{
	color: #fff;
	background:#A9A9A9;
	font-weight: bold;
	border: 1px solid #1a1a1a;
}

ul#sliding-navigation li.sliding-element a
{
	color: #fff;
	background:#A9A9A9 repeat-y;
	border: 1px solid #1a1a1a;
	text-decoration: none;
}

ul#sliding-navigation li.sliding-element a:hover { color: #f2f2f6; }

ul#sliding-navigation li.sliding-element ul li a.sub-menu-element {
	background: #696969;
}

.mainMenuDiv {
	position:absolute;
	height:180px;
	left:330px;
	top:200px;
	width:900px;
}

.mainMenuGroupDiv{
	float:left; 
	padding-left:10px;
	padding-top:20px;
	padding-right:10px;
	padding-bottom:20px
}

.mainMenuGroupItemDiv{
	padding-top:5px;
	text-align:center;
	background-color:Silver;
	margin-bottom:20;
	padding-bottom:10px;
	-moz-border-radius: 4px;
	-webkit-border-radius: 4px;
	-khtml-border-radius: 4px;
	border-radius: 4px;
}

.mainMenuGroupItemDiv a {
	margin-right:10px;
	margin-left:10px;
}
